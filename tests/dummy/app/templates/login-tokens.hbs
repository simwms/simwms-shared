<div class="col-xs-12">
  {{#if currentUser.accountLoggedIn}}
    <h1 id="success">Login Successful!</h1>
  {{else}}
    {{#em-form-for model action="submit" as |f|}}
      {{#em-text-field f name="rememberToken"}}
        <span>Remember Tokens: {{model.rememberToken}}</span>
      {{/em-text-field}}
      <button class="btn btn-lg btn-primary" type="submit" id="token">
        Submit
      </button>
    {{/em-form-for}}
  {{/if}}
</div>